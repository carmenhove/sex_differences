  shorttitle = {Age- and Sex-related Reference Intervals of Lymphocyte Subsets in Healthy Ethnic},
  author = {Jia, Liting and Li, Jing and Zhang, Yuchao and Shi, Ying and Yuan, Enwu and Liu, Junjie and Wang, Peng and Rong, Shouhua and Xing, Jinfang and Tian, Yuan and Li, Junfang},
  year = {2015},
