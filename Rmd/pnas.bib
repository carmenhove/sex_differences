@article{jansson_etal13,
  title = {Activation of {{Placental mTOR Signaling}} and {{Amino Acid Transporters}} in {{Obese Women Giving Birth}} to {{Large Babies}}},
  author = {Jansson, Nina and Rosario, Fredrick J. and Gaccioli, Francesca and Lager, Susanne and Jones, Helen N. and Roos, Sara and Jansson, Thomas and Powell, Theresa L.},
