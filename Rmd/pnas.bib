  author = {Johannsen, Trine Holm and Main, Katharina Maria and Ljubicic, Marie Lindhardt and Jensen, Tina Kold and Andersen, Helle Raun and Andersen, Marianne Skovsager and Petersen, J{\o}rgen Holm and Andersson, Anna-Maria and Juul, Anders},
