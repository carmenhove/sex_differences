@article{Blackwell2016,
  title = {Immune Function in {{Amazonian}} Horticulturalists},
  author = {Blackwell, Aaron D. and Trumble, Benjamin C. and Maldonado Suarez, Ivan and Stieglitz, Jonathan and Beheim, Bret and Snodgrass, J. Josh and Kaplan, Hillard and Gurven, Michael},
