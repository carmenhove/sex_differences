@article{jones_etal10,
  title = {Relationship between Maternal Growth, Infant Birthweight and Nutrient Partitioning in Teenage Pregnancies},
  author = {Jones, Rl and Cederberg, Hms and Wheeler, Sj and Poston, L and Hutchinson, Cj and Seed, Pt and Oliver, Rl and Baker, Pn},
  year = {2010},
